@using Shop.Areas.Administrator.Data.message
@if (Notification.has_flash())
{
    ModelNotification Notifi = Notification.get_flash();
    if (Notifi != null)
    {
        <input type="hidden" value="@Notifi.mgs_type" name="mgs_type" />
        <input type="hidden" value="@Notifi.mgs" name="mgs" />
    }
}
